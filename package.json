{
  "name": "react-drawer",
  "version": "1.3.4",
  "description": "react drawer menu",
  "main": "./lib/react-drawer.js",
  "scripts": {
    "start": "webpack --config ./webpack.dev.config.js && webpack-dev-server",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "prepare": "npm run lint && jest && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src/",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atom2ueki/react-drawer.git"
  },
  "author": "Tony Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atom2ueki/react-drawer/issues"
  },
  "homepage": "https://github.com/atom2ueki/react-drawer",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "prop-types": "^15.5.2"
  },
  "devDependencies": {
    "animate.css": "^3.5.1",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.1.4",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "jest": "^22.3.0",
    "node-sass": "^4.7.2",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "semantic-release": "^12.4.1",
    "style-loader": "^0.20.1",
    "travis-deploy-once": "^4.3.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "testRegex": "/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/src/style.mock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
