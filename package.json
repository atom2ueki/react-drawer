{
  "name": "react-drawer",
  "version": "1.2.2",
  "description": "react drawer menu",
  "main": "./lib/react-drawer.js",
  "scripts": {
    "demo": "webpack-dev-server",
    "build": "webpack",
    "lint": "./node_modules/.bin/eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublish": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atom2ueki/react-drawer.git"
  },
  "author": "Tony Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atom2ueki/react-drawer/issues"
  },
  "homepage": "https://github.com/atom2ueki/react-drawer",
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "devDependencies": {
    "animate.css": "^3.5.1",
    "babel-cli": "^6.0.0",
    "babel-core": "^6.1.4",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jest": "^14.1.0",
    "node-sass": "^3.8.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "testRegex": "/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/src/style.mock.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/fbjs/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/cheerio/",
      "<rootDir>/node_modules/htmlparser2/",
      "<rootDir>/node_modules/lodash/",
      "<rootDir>/node_modules/domhandler/",
      "<rootDir>/node_modules/object.assign/",
      "<rootDir>/node_modules/define-properties/",
      "<rootDir>/node_modules/function-bind/",
      "<rootDir>/node_modules/object-keys/",
      "<rootDir>/node_modules/object.values/",
      "<rootDir>/node_modules/es-abstract/",
      "<rootDir>/node_modules/react-jsonschema-form/",
      "<rootDir>/node_modules/classnames/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
